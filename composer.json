{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Jose\\": "common/components/translation/jwt-framework/src"
    }
  },
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-apidoc": "*",
    "yiisoft/yii2-authclient": "*",
    "yiisoft/yii2-imagine": "*",
    "yiisoft/yii2-redis": "~2.0.0",
    "kartik-v/yii2-widget-fileinput": "1.0.8",
    "kartik-v/yii2-widgets": "*",
    "kartik-v/yii2-editable": "^1.7",
    "kartik-v/yii2-date-range": "*",
    "kartik-v/yii2-field-range": "*",
    "kartik-v/yii2-grid": "*",
    "kartik-v/yii2-widget-colorinput": "*",
    "tinymce/tinymce": "4.4.0",
    "yii2tech/crontab": "*",
    "voskobovich/yii2-many-many-behavior": "^3.0",
    "leandrogehlen/yii2-treegrid": "*",
    "sendpulse/rest-api": "^1.0",
    "yiisoft/yii2-elasticsearch": "~2.0.0",
    "yandex/translate-api": "1.5.x",
    "yandex-money/yandex-checkout-sdk-php": "^1.0",
    "himiklab/yii2-recaptcha-widget": "*",
    "vlucas/phpdotenv": "^3.4",
    "spomky-labs/base64url": "^1.0|^2.0",
    "mobiledetect/mobiledetectlib": "^2.8"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*",
    "cedx/yii2-mustache": "^0.5.2"
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    },
    "process-timeout": 1800
  },
  "extra": {
  },
  "scripts": {
    "post-install-cmd": "php init --env=Development --overwrite=n"
  }
}
